@article{Cade2003,
  Author =	 {Cade, B. S. and Noon, B. R.},
  Title =	 {{A gentle introduction to quantile regression for
                  ecologists}},
  Journal =	 {{Frontiers in Ecology and the Environment}},
  Year =	 {{2003}},
  Volume =	 {{1}},
  Number =	 {{8}},
  Pages =	 {{412-420}},
  Abstract =	 {{Quantile regression is a way to estimate the
                  conditional quantiles of a response variable
                  distribution in the linear model that provides a
                  more complete view of possible causal relationships
                  between variables in ecological
                  processes. Typically, all the factors that affect
                  ecological processes are not measured and included
                  in the statistical models used to investigate
                  relationships between variables associated with
                  those processes. As a consequence, there may be a
                  weak or no predictive relationship between the mean
                  of the response variable (y) distribution and the
                  measured predictive factors (X). Yet there may be
                  stronger, useful predictive relationships with other
                  parts of the response variable distribution. This
                  primer relates quantile regression estimates to
                  prediction intervals in parametric error
                  distribution regression models (eg least squares),
                  and discusses the ordering characteristics, interval
                  nature, sampling variation, weighting, and
                  interpretation of the estimates for homogeneous and
                  heterogeneous regression models.}},
  DOI =		 {{10.2307/3868138}},
  ISSN =	 {{1540-9295}},
  Unique-ID =	 {{ISI:000221791500016}},
}

